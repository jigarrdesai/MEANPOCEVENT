<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>POC EVENT</title>

</head>
<body>
	<div id="wrapper">
		<div id="page-wrapper">
			<div class="row">
				<div class="col-lg-12">
					<h1 class="page-header"></h1>
				</div>
			</div>
			<ui-view></ui-view>
		</div>
	</div>

	<% for(m in angular.modules) { var mod = angular.modules[m]; %>
	    <script type="text/javascript" src="/modules/<%= mod %>"></script>
	<% } %>
	<% for(f in angular.factories) { var fact = angular.factories[f]; %>
	    <script type="text/javascript" src="/factories/<%= fact %>"></script>
	<% } %>
	<% for(f in angular.directives) { var fact = angular.directives[f]; %>
	    <script type="text/javascript" src="/directives/<%= fact %>"></script>
	<% } %>
	<% for(c in angular.controllers) { var ctrl = angular.controllers[c]; %>
	    <script type="text/javascript" src="/controllers/<%= ctrl %>"></script>
	<% } %>
	<% for(c in angular.filters) { var ctrl = angular.filters[c]; %>
	    <script type="text/javascript" src="/filters/<%= ctrl %>"></script>
	<% } %>
</body>
</html>
